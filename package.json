{
  "name": "vue-wordpress-pwa",
  "version": "0.0.0",
  "description": "An offline-first SPA using Vue.js, the WordPress REST API and Progressive Web Apps",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bstavroulakis/vue-wordpress-pwa"
  },
  "engines": {
    "npm": "3.10.7"
  },
  "keywords": [
    "vue",
    "vuex",
    "offline",
    "progressive-web-app",
    "pwa-apps",
    "pwa",
    "webpack",
    "starter",
    "server-side",
    "boilerplate"
